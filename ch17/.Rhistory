three_d_point_generator <- setRefClass(
"three_d_point",
fields = list(
z = "numeric"
),
contains = "point", #this line lets us inherit
methods = list(
initialize = function(x, y, z)
{
"Assign x and y upon object creation."
x <<- x
y <<- y
z <<- z
}
)
)
