divider <- function(numerator, denominator){
if(denominator == 0)  {
denominator_name <- deparse(substitute(denominator))
warning("The denominator, ", sQuote(denominator_name), ", is zero.")
}
numerator / denominator
}
top <- 3
bottom <- 0
divider(top, bottom)
